   <children>
      <Label fx:id="titleLabel" layoutX="50.0" layoutY="60.0" prefHeight="25.0" prefWidth="30.0" text="Title" />
      <TextField fx:id="titleTextField" layoutX="80.0" layoutY="60.0" prefHeight="25.0" prefWidth="150.0" />
      <Button fx:id="createButton" layoutX="205.0" layoutY="100.0" mnemonicParsing="false" onAction="#createPlaylist" text="Create" />
   </children>
</AnchorPane>
